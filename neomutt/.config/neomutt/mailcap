text/plain; sensible-editor %s;
text/html; sensible-browser %s &; test=test -n "$DISPLAY"; needsterminal;
# text/html; w3m -I %{charset} -T text/html; copiousoutput;
text/html; lynx -assume_charset=%{charset} -display_charset=utf-8 -dump %s; nametemplate=%s.html; copiousoutput;
text/calendar; khal import %s;
application/ics; khal import %s;
# text/calendar; vkhal -s %s; copiousoutput
# application/ics; vkhal -s %s; copiousoutput
# text/calendar; /path/to/vcalendar-filter; copiousoutput

application/pdf; xdg-open %s;
application/pgp-encrypted; gpg -d '%s'; copiousoutput;

application/vnd.oasis.opendocument.text; libreoffice %s
application/vnd.oasis.opendocument.spreadsheet; libreoffice %s
application/vnd.openxmlformats-officedocument.wordprocessingml.document; libreoffice %s
application/msword; libreoffice %s

video/*; setsid mpv --quiet %s &; copiousoutput
audio/*; mpv %s ;

# image/*; xdg-open %s;
image/*; feh %s
